{
  "name": "fuzzball",
  "version": "0.2.23",
  "description": "Fuzzy string matching algorithms and utilities, port of the fuzzywuzzy Python library.",
  "main": "fuzzball.js",
  "files": [
    "fuzzball.js",
    "dist/fuzzball_browser.js",
    "dist/fuzzball_browser.min.js",
    "test/test.js",
    "test/testdata.js",
    "lite/fuzzball_lite.js",
    "lite/fuzzball_lite_browser.min.js"
  ],
  "dependencies": {
    "damlev": ">=1.0.0",
    "difflib": ">=0.2.4",
    "heap": ">=0.2.0",
    "lodash.difference": "^4.5.0",
    "lodash.foreach": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.isarray": "^4.0.0",
    "lodash.keys": "^4.2.0",
    "lodash.uniq": "^4.5.0"
  },
  "devDependencies": {
    "browserify": "^1.0.0",
    "mocha": "^3.2.0",
    "uglify-js": "^1.0.0",
    "lodash-cli": "^4.17.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nol13/fuzzball.js.git"
  },
  "keywords": [
    "levenshtein",
    "string",
    "fuzzy",
    "matching",
    "search",
    "ratio",
    "distance",
    "compare"
  ],
  "author": "Nolan Kaplan <nolan@nolankaplan.com>",
  "license": "GPL-2.0",
  "scripts": {
    "lodash:lite": "./node_modules/lodash-cli/bin/lodash include=intersection,difference,uniq -p -o ./lite/lodash.custom.min.js",
    "browserify:lite": "browserify -r ./lite/fuzzball_lite.js:fuzzball > ./lite/fuzzball_lite_browser.js",
    "uglify:lite": "uglifyjs ./lite/fuzzball_lite_browser.js > ./lite/fuzzball_lite_browser.min.js",
    "uglify": "uglifyjs ./dist/fuzzball_browser.js > ./dist/fuzzball_browser.min.js",
    "browserify": "browserify -r ./fuzzball.js:fuzzball > ./dist/fuzzball_browser.js",
    "build:reg": "npm run browserify && npm run uglify",
    "build:lite": "npm run lodash:lite && npm run browserify:lite && npm run uglify:lite",
    "build": "npm run build:reg && npm run build:lite",
    "test": "mocha"
  }
}
