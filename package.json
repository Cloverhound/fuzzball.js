{
  "name": "fuzzball",
  "version": "0.9.0",
  "description": "Fuzzy string matching algorithms and utilities, port of the fuzzywuzzy Python library.",
  "main": "fuzzball.umd.min.js",
  "files": [
    "fuzzball.umd.min.js",
    "dist",
    "lite",
    "lib",
    "test",
    "src",
    "docs"
  ],
  "dependencies": {
    "difflib": ">=0.2.4",
    "heap": ">=0.2.0",
    "lodash.difference": "^4.5.0",
    "lodash.foreach": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.isarray": "^4.0.0",
    "lodash.keys": "^4.2.0",
    "lodash.toarray": "^4.4.0",
    "lodash.uniq": "^4.5.0",
    "setimmediate": "^1.0.5",
    "string.fromcodepoint": "^0.2.1",
    "string.prototype.codepointat": "^0.2.0"
  },
  "devDependencies": {
    "browserify": "^14.0.0",
    "mocha": "^3.2.0",
    "uglify-js": "^1.0.0",
    "lodash-cli": "^4.17.4",
    "nsp": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nol13/fuzzball.js.git"
  },
  "keywords": [
    "levenshtein",
    "string",
    "fuzzy",
    "matching",
    "search",
    "ratio",
    "distance",
    "compare"
  ],
  "author": "Nolan Kaplan <nolan@nolankaplan.com>",
  "license": "GPL-2.0",
  "scripts": {
    "jsdoc": "jsdoc src/fuzzball.js -d docs",
    "lodash:lite": "./node_modules/lodash-cli/bin/lodash include=intersection,difference,uniq,toArray -p -o ./lite/lodash.custom.min.js",
    "build:reg:old": "browserify -r ./src/fuzzball.js:fuzzball | uglifyjs > dist/fuzzball_browser.min.js",
    "build:lite:old": "browserify -r ./lite/fuzzball_lite.js:fuzzball | uglifyjs > lite/fuzzball_lite_browser.min.js",
    "build:reg": "browserify src/fuzzball.js --standalone fuzzball | uglifyjs > fuzzball.umd.min.js",
    "build:lite": "browserify lite/fuzzball_lite.js --standalone fuzzball | uglifyjs > lite/fuzzball_lite.umd.min.js",
    "build": "npm run lodash:lite && npm run build:reg && npm run build:lite && npm run build:reg:old && npm run build:lite:old && npm run jsdoc && npm run test:build",
    "test": "mocha && npm run nsp",
    "test:build": "testenv=build npm run test",
    "nsp": "node_modules/nsp/bin/nsp check"
  },
  "runkitExample": "var fuzz = require('fuzzball');"
}
